using System;

namespace ConsoleApp4
{
    class Program
    {
        static void Main(string[] args)
        {
         int a = 1;
int b = 6;
int c = -7;

double delta = (b * b) - (4 * a * c);
double raiz = Math.Sqrt(delta);
var a1 = ((-(b)) + raiz) / (2 * a);
var a2 = ((-(b)) - raiz) / (2 * a);

Console.WriteLine("Valor de a1: {0}", a1);
Console.WriteLine("Valor de a2: {0}", a2);


        }
    }
}